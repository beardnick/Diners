<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asus.diners.Fragment.FootPrintFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/view_myrecentorders"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="25sp"
        android:textColor="#000"
        android:textStyle="bold"
        android:text="最近三个月还没有订单哦！"/>

    <ImageView
        android:id="@+id/view_myorder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/myorder"/>


    <Button
        android:id="@+id/button_watchmore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:text="查看更多"/>


</FrameLayout>
